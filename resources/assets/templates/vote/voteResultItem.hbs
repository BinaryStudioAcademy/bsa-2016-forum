<div>
    <div>{{ model.name }}</div>
    <div class="progress">
        <div class="progress-bar progress-bar-warning" role="progressbar"
             aria-valuenow="{{ votePercent }}" aria-valuemin="0" aria-valuemax="100"
             style="width: {{ votePercent }}%">
            <span>
                {{ votePercent }}%
            </span>
        </div>
    </div>
    {{#if isPublic}}
        <div class="vote-item-users">
            <ul class="list-inline list-unstyled">
                {{#each meta.users}}
                    <!-- user avatar -->
                    <li><a href=""><img src="/images/user.png" alt="{{ display_name }}"></a></li>
                {{/each}}
            </ul>
        </div>
    {{/if}}
</div>
